{
  "extends": [
    "@rocketseat/eslint-config/react"
  ],
  "rules": {
    "camelcase": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-var-requires": "off",
    "react/display-name": "off",
    "react-hooks/exhaustive-deps": "off"
  },
  "plugins": [
    "react-hooks"
  ]
}